<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="#fff2f8ff" android:endColor="#fff2f8ff" android:angle="90.0" />
        </shape>
    </item>
    <item android:gravity="center|left|top">
        <bitmap android:gravity="center|left|top" android:src="@drawable/app_loading_top_left" android:filter="true" />
    </item>
    <item android:gravity="center|right|top">
        <bitmap android:gravity="center|right|top" android:src="@drawable/app_loading_top_right" android:filter="true" />
    </item>
    <item android:gravity="bottom|center|left">
        <bitmap android:gravity="bottom|center|left" android:src="@drawable/app_loading_bottom_left" android:filter="true" />
    </item>
    <item android:gravity="bottom|center|right">
        <bitmap android:gravity="bottom|center|right" android:src="@drawable/app_loading_bottom_right" android:filter="true" />
    </item>
    <item android:gravity="bottom" android:bottom="18.0dip">
        <bitmap android:gravity="center_horizontal" android:src="@drawable/icon_blued_banner_logo" android:filter="true" />
    </item>
</layer-list>