<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <solid android:color="@color/syc_tran30_999999" />
            <corners android:radius="@dimen/dp_100" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <gradient android:startColor="@color/syc_00E0AB" android:endColor="@color/syc_3883FD" android:angle="0.0" android:type="linear" />
                <corners android:radius="@dimen/dp_100" />
            </shape>
        </clip>
    </item>
</layer-list>